local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Cutsences Scripts", HidePremium = false, SaveConfig = true, ConfigFolder = "eatFat"})




local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "General"
})

Tab:AddButton({
	Name = "New UI",
	Callback = function()
        loadstring(game:HttpGet(('"https://raw.githubusercontent.com/Cutsences/SteepSteps/main/SteepSteps.lua')))()
        OrionLib:Destroy()
  	end    
})




Tab:AddButton({
	Name = "Old UI",
	Callback = function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/Cutsences/SteepSteps/main/old.lua')))()
        OrionLib:Destroy()
  	end    
})



OrionLib:Init()
